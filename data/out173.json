[
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "云惨烟愁苑路斜。", 
            "朱铅滴尽无心语。"
        ], 
        "title": "集古 悲故宫人", 
        "type": "绝句", 
        "yun": "麻"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "暮云宫阙古今情。", 
            "春意自知无主惜。"
        ], 
        "title": "集古 怀古", 
        "type": "绝句", 
        "yun": "真"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "山南山北雨濛濛。", 
            "今日独来张乐地。"
        ], 
        "title": "集古 旧游", 
        "type": "绝句", 
        "yun": "东"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "岳北秋空渭北川。", 
            "行人莫讶频回首。"
        ], 
        "title": "集古 关中", 
        "type": "绝句", 
        "yun": "先"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "远到音书转寂寥。", 
            "村园门巷多相似。"
        ], 
        "title": "集古 过梅溪旧居", 
        "type": "绝句", 
        "yun": "萧"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "将军一去泣空营。", 
            "试上高楼望春色。"
        ], 
        "title": "集古 登山阳郡楼", 
        "type": "绝句", 
        "yun": "庚"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "诗酒能消一半春。", 
            "黄河九曲今归汉。"
        ], 
        "title": "集古 王公楼上会饮", 
        "type": "绝句", 
        "yun": "庚"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "萧萧风竹夜窗寒。", 
            "对酒已成千里客。"
        ], 
        "title": "集古 送客之南宫", 
        "type": "绝句", 
        "yun": "删"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "一夕秋风白发生。", 
            "前欢往恨分明在。"
        ], 
        "title": "集古 荅筹堂", 
        "type": "绝句", 
        "yun": "庚"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "金凤罗衣湿麝薰。", 
            "傍人未必知心事。"
        ], 
        "title": "集古 夜集", 
        "type": "绝句", 
        "yun": "文"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "春风何处有佳期。", 
            "料得也应怜宋玉。"
        ], 
        "title": "集古 戏书", 
        "type": "绝句", 
        "yun": "支"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "事去人亡迹自留。", 
            "如今暂寄尊前笑。"
        ], 
        "title": "集古 重九", 
        "type": "绝句", 
        "yun": "尤"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "露叶如啼欲恨谁。", 
            "芳尊有酒无人共。"
        ], 
        "title": "集古 十日遣兴", 
        "type": "绝句", 
        "yun": "支"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "东风渐急夕阳斜。", 
            "肠断入城芳草路。"
        ], 
        "title": "集古 古城", 
        "type": "绝句", 
        "yun": "麻"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "天涯方叹异乡身。", 
            "欲问孤鸿向何处。"
        ], 
        "title": "集古 送行", 
        "type": "绝句", 
        "yun": "真"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "寒食东风御柳斜。", 
            "试看三月春残后。"
        ], 
        "title": "集古 宫柳", 
        "type": "绝句", 
        "yun": "麻"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "尽是刘郎去后栽。", 
            "风流才子多春思。"
        ], 
        "title": "集古 对花", 
        "type": "绝句", 
        "yun": "灰"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "远上寒山石径斜。", 
            "故乡今夜思千里。"
        ], 
        "title": "集古 宿横望", 
        "type": "绝句", 
        "yun": "麻"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "无地无媒只一身。", 
            "春来削发芙蓉寺。"
        ], 
        "title": "集古 妓为尼", 
        "type": "绝句", 
        "yun": "真"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "每忆云山养短才。", 
            "落花寂寂黄昏雨。"
        ], 
        "title": "集古 新居", 
        "type": "绝句", 
        "yun": "灰"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "浮云飞尽日西颓。", 
            "独上郊原人不见。"
        ], 
        "title": "集古 寒食野外", 
        "type": "绝句", 
        "yun": "灰"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "绿柳才黄半未匀。", 
            "相逢且莫推辞酒。"
        ], 
        "title": "集古 赠别", 
        "type": "绝句", 
        "yun": "真"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "还到春时别恨生。", 
            "自家飞絮犹无定。"
        ], 
        "title": "集古 柳", 
        "type": "绝句", 
        "yun": "真"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "自有林亭不得闲。", 
            "晚来风起花如雪。"
        ], 
        "title": "集古 惜春", 
        "type": "绝句", 
        "yun": "寒"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "半似羞人半忍寒。", 
            "门前不见归轩迹。"
        ], 
        "title": "集古 春望", 
        "type": "绝句", 
        "yun": "寒"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "风景依稀似去年。", 
            "夜深斜搭鞦韆索。"
        ], 
        "title": "集古 寒食夜雨", 
        "type": "绝句", 
        "yun": "先"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "一声歌尽各东西。", 
            "马上政吟归去好。"
        ], 
        "title": "集古 早行", 
        "type": "绝句", 
        "yun": "齐"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "少游京洛共红尘。", 
            "且向白云求一醉。"
        ], 
        "title": "集古 过友人别墅", 
        "type": "绝句", 
        "yun": "真"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "此去秦关路几多。", 
            "荷蓑不是人閒事。"
        ], 
        "title": "集古 隐居", 
        "type": "绝句", 
        "yun": "歌"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "忽闻春尽强登山。", 
            "惆怅仙翁何处去。"
        ], 
        "title": "集古 访隐者不遇", 
        "type": "绝句", 
        "yun": "删"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "来往风尘共白头。", 
            "直教桂子落坟上。"
        ], 
        "title": "集古 不遇 其一", 
        "type": "绝句", 
        "yun": "尤"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "岂有文章动圣君。", 
            "平生名利关身者。"
        ], 
        "title": "集古 不遇 其二", 
        "type": "绝句", 
        "yun": "文"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "乌巾年少归何处。", 
            "上天下地鹤一只。"
        ], 
        "title": "集古 怀韩居士", 
        "type": "绝句", 
        "yun": "支"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "鹤骨飘飘紫府仙。", 
            "无人寂寂春山路。"
        ], 
        "title": "集古 古道人", 
        "type": "绝句", 
        "yun": "先"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "雾为襟袖玉为冠。", 
            "上界真人足官府。"
        ], 
        "title": "集古 游仙", 
        "type": "绝句", 
        "yun": "删"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "麻衣年少雪为颜。", 
            "弄玉已归萧史去。"
        ], 
        "title": "集古 李道者", 
        "type": "绝句", 
        "yun": null
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "上清真子玉童颜。", 
            "云雨今归何处去。"
        ], 
        "title": "集古 女仙台", 
        "type": "绝句", 
        "yun": "删"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "蝉鬓慵梳倚帐门。", 
            "不应更学文君去。"
        ], 
        "title": "集古 听琴", 
        "type": "绝句", 
        "yun": "真"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "桂泠松香十里閒。", 
            "年年笑伴皆归去。"
        ], 
        "title": "集古 仙庙 其一", 
        "type": "绝句", 
        "yun": "删"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "殿台浑不似人寰。", 
            "高坐寂寥尘漠漠。"
        ], 
        "title": "集古 仙庙 其二", 
        "type": "绝句", 
        "yun": "删"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "二十馀年别帝京。", 
            "秋光何处堪消日。"
        ], 
        "title": "集古 重九日", 
        "type": "绝句", 
        "yun": "庚"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "一任斜阳送客愁。", 
            "自从身逐征西府。"
        ], 
        "title": "集古 九日戏幕宾", 
        "type": "绝句", 
        "yun": "尤"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "白露寒花自绕篱。", 
            "一年秋色吟中过。"
        ], 
        "title": "集古 晚菊", 
        "type": "绝句", 
        "yun": "支"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "篱菊香寒晚吹笙。", 
            "浮生也共花无别。"
        ], 
        "title": "集古 十日对菊", 
        "type": "绝句", 
        "yun": "元"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "我今移尔满庭栽。", 
            "回首看花花欲尽。"
        ], 
        "title": "集古 梅", 
        "type": "绝句", 
        "yun": "灰"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "每到花时把酒杯。", 
            "纵然一夜风吹去。"
        ], 
        "title": "集古 落梅", 
        "type": "绝句", 
        "yun": "灰"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "古沟芳草起寒云。", 
            "万里江山今不闭。"
        ], 
        "title": "集古 塞上", 
        "type": "绝句", 
        "yun": "文"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "东池送客醉年华。", 
            "日暮鸟啼人散尽。"
        ], 
        "title": "集古 登楼", 
        "type": "绝句", 
        "yun": "麻"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "云泥岂合得相亲。", 
            "一种是春长富贵。"
        ], 
        "title": "集古 花期不赴", 
        "type": "绝句", 
        "yun": "真"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "糁径杨花铺白毡。", 
            "满山明月东风夜。"
        ], 
        "title": "集古 醉眠", 
        "type": "绝句", 
        "yun": "先"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "弃掷功名脱屣閒。", 
            "行人莫话金章贵。"
        ], 
        "title": "集古 山中偶得", 
        "type": "绝句", 
        "yun": "删"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "虚榻吟窗更待谁。", 
            "明朝骑马摇鞭去。"
        ], 
        "title": "集古 夜饮", 
        "type": "绝句", 
        "yun": "支"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "试将小录问同年，风采依稀坠目前。", 
            "三十一人今鬼录，与君虽在各华颠。"
        ], 
        "title": "寄赵庭干二首 其一", 
        "type": "绝句", 
        "yun": "先"
    }, 
    {
        "author": "李俊民", 
        "era": "金", 
        "subtype": "七言绝句", 
        "text": [
            "君还携幼去幽燕，我向荒山学种田。", 
            "千里暮鸿行断处，碧云容易作愁天。"
        ], 
        "title": "寄赵庭干二首 其二", 
        "type": "绝句", 
        "yun": "先"
    }
]